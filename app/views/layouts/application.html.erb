          <div class="nav-right nav-wrap"><div class="w-dropdown" data-delay="0">
            <div class="dropdown-toggle-3 nav-link w-dropdown-toggle">
              <div class="w-icon-dropdown-toggle"></div>
              <div class="nav-user"><%= current_user.email %></div>
            </div>
            <nav class="dropdown-list-2 w-dropdown-list"><%= link_to('Settings', edit_user_registration_path, class: "dropdown-link-4 w-dropdown-link") %><a class="dropdown-link-4 w-dropdown-link" href="#">Observation list</a><%= link_to('Logout', destroy_user_session_path, method: :delete, class: "dropdown-link-4 w-dropdown-link") %></nav>
          </div></div>
        <% else %>
        <div class="nav-right nav-wrap"><a class="nav-link w-nav-link" href="<%= new_user_registration_path %>">SIGN UP</a><a class="nav-link w-nav-link" href="<%= new_user_session_path %>">LOG IN</a></div>
        <% end %>
      </nav>
      <div class="menu-button w-nav-button">
        <div class="icon w-icon-nav-menu"></div>
      </div>
    </div>
  </div>
  <div class="navbar-2 w-nav" data-animation="default" data-collapse="medium" data-duration="400">
