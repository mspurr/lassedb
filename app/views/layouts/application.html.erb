  <body class="body">
    <div class="navbar w-nav" data-animation="default" data-collapse="small" data-duration="400">
    <div class="w-container">
      <nav class="nav-menu-2 w-clearfix w-nav-menu" role="navigation"><a class="w-nav-brand" href="#"></a>
        <div class="mobile-nav nav-left nav-wrap"><a class="mobile-nav-link nav-link w-nav-link" href="<%= companies_path %>">COMPANIES</a><a class="mobile-nav-link nav-link w-nav-link" href="#">BROWSE FOCUS AREAS</a><a class="mobile-nav-link nav-link w-nav-link" href="<%= new_user_registration_path %>">SIGN UP</a><a class="mobile-nav-link nav-link w-nav-link" href="<%= new_user_session_path %>">LOG IN</a></div>
        <div class="nav-left nav-wrap"><a class="nav-link w-nav-link" href="<%= companies_path %>">COMPANIES</a><a class="nav-link w-nav-link" href="#">BROWSE FOCUS AREAS</a></div>
